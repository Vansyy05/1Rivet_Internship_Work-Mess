<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    
    <hibernate-mapping>
    <class name="com.onerivet.User" table="details">
    <id name="id" column="studentdt" type="int">
    <generator class="identity"/>
    </id>
    <property name="acc_no" type="long"></property>
    <property name="name" type="string"></property>
    <property name="phone_no" type="long"></property>
    <property name="cramount" type="long"></property>
    </class>
    </hibernate-mapping>